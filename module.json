{
  "name": "fvtt-party-resources",
  "title": "Party Resources",
  "description": "Create and track custom party resources.",
  "authors": [
    {
        "name": "Dave Lens"
    }
],
  "url": "https://github.com/davelens/fvtt-party-resources",
  "manifest": "https://github.com/davelens/fvtt-party-resources/releases/latest/download/module.json",
  "download": "https://github.com/davelens/fvtt-party-resources/releases/download/1.4.2/module.zip",
  "version": "1.4.2",
  "minimumCoreVersion": "9",
  "compatibleCoreVersion": "10",
  "compatibility": {
    "minimum": "9",
    "verified": "10",
    "maximum": "10"
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    },
    {
      "lang": "cn",
      "name": "中文",
      "path": "languages/cn.json"
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "languages/fr.json"
    },
    {
      "lang": "ja",
      "name": "日本語",
      "path": "languages/ja.json"
    },
    {
      "lang": "nl",
      "name": "Nederlands",
      "path": "languages/nl.json"
    }
  ],
  "esmodules": [
    "./src/init.mjs"
  ],
  "styles": [
    "./styles/css/init.css"
  ],
  "relationships": {
    "requires": [
        {
            "id": "settings-extender",
            "type": "module"
        }
    ]
},
  "socket": true
}
